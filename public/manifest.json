{
  "manifest_version": 3,
  "name": "Ignite URL switch",
  "description": "Switch the Ignite running environment with one click.",
  "version": "1.0.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "24": "icon.png",
      "32": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": ["storage", "tabs"]
}
